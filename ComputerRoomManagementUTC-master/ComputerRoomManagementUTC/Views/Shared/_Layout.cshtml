                        <a href="@Url.Action("Index", "Home")">HASAGI</a>
                    </div>
                    <div class="sidebar-user">
                        <div class="sidebar-user-picture">
                            <img alt="image" src="~/img/avatar/avatar-1.jpeg">
                        </div>
                        <div class="sidebar-user-details">
                            <div class="user-name">@Context.Session.GetString("UserName")</div>
                            <div class="user-role">
                                Administrator
                            </div>
                        </div>
                    </div>
