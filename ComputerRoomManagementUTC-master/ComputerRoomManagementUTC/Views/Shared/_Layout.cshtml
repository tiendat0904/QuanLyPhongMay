                                        <li><a href="@Url.Action("SpeedPaging", "Speed", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Speed</a></li>
                                        <li><a href="@Url.Action("MousePaging", "Mouse", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Mouse</a></li>
                                        <li><a href="@Url.Action("KeyboardPaging", "Keyboard", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Keyboard</a></li>
                                        <li><a href="@Url.Action("StorageDevicePaging", "StorageDevice", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Storage Device</a></li>
                                        <li><a href="@Url.Action("StorageCapacityPaging", "StorageCapacity", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Storage Device Capacity</a></li>
                                        <li><a href="@Url.Action("OpticalDiscPaging", "OpticalDisc", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Optical Disc</a></li>
                                    </ul>
                                </li>

                            </ul>
                        </li>
                        <li>
                            <a href="#" class="has-dropdown"><i class="ion ion-ios-albums-outline"></i><span>Schedule</span></a>
                            <ul class="menu-dropdown">
                                <li><a href="@Url.Action("StudySchedulePaging", "StudySchedule", new { PAGE_INDEX = 1, PAGE_SIZE = 10 })"><i class="ion ion-ios-circle-outline"></i>Manage Study Schedule</a></li>
                                <li>
                                    <a href="#" class="has-dropdown"><i class="ion ion-ios-circle-outline"></i>Manage Human</a>
                                    <ul class="menu-dropdown">
                                        <li><a href="@Url.Action("ClassPaging", "Class", new { PAGE_INDEX = 1, PAGE_SIZE = 10 })"><i class="ion ion-ios-circle-outline"></i>Manage Classes</a></li>
                                        <li><a href="@Url.Action("StudentPaging", "Student", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Manage Students</a></li>
                                        <li><a href="@Url.Action("TeacherPaging", "Teacher", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Manage Teachers</a></li>
                                        <li><a href="@Url.Action("EmployeePaging", "Employee", new { PAGE_INDEX = 1, PAGE_SIZE = 10})"><i class="ion ion-ios-circle-outline"></i>Manage Employees</a></li>
                                    </ul>
                                </li>

                            </ul>
                        </li>
                        <li>
                            <a href="#" class="has-dropdown"><i class="ion ion-ios-albums-outline"></i><span>Manage Basic Data</span></a>
                            <ul class="menu-dropdown">
                                <li><a href="@Url.Action("ProvincePaging", "Province", new { PAGE_INDEX = 1, PAGE_SIZE = 10 })"><i class="ion ion-ios-circle-outline"></i>Manage Province</a></li>
                                <li><a href="@Url.Action("ShiftPaging", "Shift", new { PAGE_INDEX = 1, PAGE_SIZE = 10 })"><i class="ion ion-ios-circle-outline"></i>Manage Shift</a></li>
                                <li><a href="@Url.Action("SubjectPaging", "Subject", new { PAGE_INDEX = 1, PAGE_SIZE = 10 })"><i class="ion ion-ios-circle-outline"></i>Manage Subject</a></li>

                            </ul>
                        </li>
