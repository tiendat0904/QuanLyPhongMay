    <script src="~/js/custom.js"></script>
    <script src="~/js/demo.js"></script>


    <script src="https://cdn.jsdelivr.net/npm/select2@4.0.11/dist/js/select2.min.js"></script>
    <script src="~/lib/masterData.js"></script>
